{
  "sideEffects": false,
  "engines": {
    "node": "20.10.0",
    "npm": "9.5.0"
  },
  "scripts": {
    "build": "remix build",
    "build:sourcemap": "remix build --sourcemap",
    "sentry:upload_sourcemaps": "node ./node_modules/@sentry/remix/scripts/sentry-upload-sourcemaps.js",
    "delete_sourcemaps": "find ./public/build -type f -name '*.map' -delete && find ./build -type f -name '*.map' -delete",
    "dev": "remix build && run-p \"dev:*\"",
    "dev:node": "cross-env NODE_ENV=development nodemon --require dotenv/config --require newrelic ./server.js --watch ./server.js",
    "dev:remix": "remix watch",
    "start": "cross-env NODE_ENV=production node -r newrelic -r ts-node/register ./server.js",
    "typecheck": "tsc",
    "prepare": "husky install",
    "lint": "npm run eslint && npm run prettier:check",
    "eslint": "eslint .",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "test": "NODE_ENV=test jest"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,css,html,md,json,yml}": "prettier --write"
  },
  "jest": {
    "clearMocks": true,
    "resetMocks": false,
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.ts"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(ts|tsx|js|jsx)$": "ts-jest"
    }
  },
  "dependencies": {
    "@codemirror/lang-javascript": "^6.2.2",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.5",
    "@googlemaps/js-api-loader": "^1.16.6",
    "@hookform/resolvers": "^3.9.0",
    "@lezer/highlight": "^1.2.0",
    "@mona-health/react-input-mask": "^3.0.3",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.5",
    "@newrelic/winston-enricher": "^4.0.1",
    "@remix-run/express": "^1.19.3",
    "@remix-run/node": "^1.19.3",
    "@remix-run/react": "^1.19.3",
    "@remix-validated-form/with-zod": "^2.0.7",
    "@sentry/remix": "^7.118.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@tanstack/react-query": "^5.50.1",
    "@verifiedinc/core-types": "github:VerifiedInc/core-types#0.47.0",
    "@verifiedinc/shared-ui-elements": "^0.3.0",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "codemirror": "^6.0.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "driver.js": "^1.3.1",
    "express": "^4.19.2",
    "helmet": "^6.2.0",
    "immer": "^10.1.1",
    "isbot": "^3.8.0",
    "knex": "^2.5.1",
    "libphonenumber-js": "^1.11.4",
    "lodash": "^4.17.21",
    "logrocket": "^3.0.1",
    "morgan": "^1.10.0",
    "newrelic": "^11.23.0",
    "pg": "^8.12.0",
    "react": "^18.3.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "react-imask": "^7.6.1",
    "redis": "^4.6.15",
    "sharp": "^0.33.4",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "tiny-invariant": "^1.3.3",
    "tinycolor2": "^1.6.0",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.19.3",
    "@remix-run/eslint-config": "^1.19.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/redis": "^4.0.11",
    "@types/tinycolor2": "^1.4.6",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@unumid/eslint-config-react": "github:UnumID/eslint-config-react#v1.0.4",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.4.0",
    "eslint-plugin-react": "^7.34.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^13.3.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  },
  "overrides": {
    "semver": "^7.5.3",
    "optionator": "0.9.3"
  }
}
